{
    "name": "fivelab/authorize-action",
    "description": "The library for authorize action before executing.",
    "keywords": [ "security", "action", "authorize", "authorize action" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Zhuk",
            "email": "v.zhuk@fivelab.org"
        },

        {
            "name": "FiveLab Team",
            "email": "dev@fivelab.org",
            "homepage": "http://fivelab.org"
        }
    ],

    "require": {
        "php": ">=7.1",
        "symfony/security": "~3.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "phpmetrics/phpmetrics": "^2.0",
        "escapestudios/symfony2-coding-standard": "~2.0"
    },

    "autoload": {
        "psr-4": { "FiveLab\\Component\\AuthorizeAction\\": "src/" }
    },

    "autoload-dev": {
        "psr-4": { "FiveLab\\Component\\AuthorizeAction\\Tests\\": "tests/" }
    },

    "config": {
        "bin-dir": "./bin/"
    }
}
